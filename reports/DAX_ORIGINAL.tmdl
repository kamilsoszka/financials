	/// Selected segment name for display purposes
	measure Segment =
			
			VAR __DISTINCT_VALUES_COUNT =
			    DISTINCTCOUNT ( 'financials'[Segment] )
			VAR __MAX_VALUES_TO_SHOW = 5
			RETURN
			    IF (
			        __DISTINCT_VALUES_COUNT > __MAX_VALUES_TO_SHOW,
			        CONCATENATE (
			            CONCATENATEX (
			                TOPN (
			                    __MAX_VALUES_TO_SHOW,
			                    VALUES ( 'financials'[Segment] ),
			                    'financials'[Segment], ASC
			                ),
			                'financials'[Segment],
			                ", ",
			                'financials'[Segment], ASC
			            ),
			            ", etc."
			        ),
			        CONCATENATEX (
			            VALUES ( 'financials'[Segment] ),
			            'financials'[Segment],
			            ", ",
			            'financials'[Segment], ASC
			        )
			    )
		displayFolder: üè∑Ô∏è Context Labels
		lineageTag: 1dd4b33e-fce8-4a4c-86d1-1aeafb12c7f3

		extendedProperty MeasureTemplate = {"version":0,"daxTemplateName":"ConcatenateValues"}

		changedProperty = FormatString

	/// Custom tooltip content with product, segment, and measure details
	measure Tooltip =
			
			VAR ProductText = "Displayed Product: " & [Product]
			VAR SegmentText = "Displayed Segment: " & [Segment] // Format measure with K/M units (2 decimal places)
			VAR MeasureText =
			    VAR MeasureValue = [Measure]
			    VAR FormattedMeasure =
			        SWITCH (
			            TRUE (),
			            ABS ( MeasureValue ) < 1000, FORMAT ( MeasureValue, "0" ),
			            ABS ( MeasureValue ) < 10000, FORMAT ( MeasureValue, "#,##0" ),
			            ABS ( MeasureValue ) < 1000000,
			                FORMAT ( MeasureValue / 1000, "0.00" ) & " K",
			            ABS ( MeasureValue ) < 10000000,
			                FORMAT ( MeasureValue / 1000000, "0.00" ) & " M",
			            FORMAT ( MeasureValue / 1000000, "0.0" ) & " M"
			        )
			    RETURN
			        "Selected Measure: " & FormattedMeasure // Format total with K/M units (2 decimal places)
			VAR TotalText =
			    VAR TotalValue = [Total]
			    VAR FormattedTotal =
			        SWITCH (
			            TRUE (),
			            ABS ( TotalValue ) < 1000, FORMAT ( TotalValue, "0" ),
			            ABS ( TotalValue ) < 10000, FORMAT ( TotalValue, "#,##0" ),
			            ABS ( TotalValue ) < 1000000,
			                FORMAT ( TotalValue / 1000, "0.00" ) & " K",
			            ABS ( TotalValue ) < 10000000,
			                FORMAT ( TotalValue / 1000000, "0.00" ) & " M",
			            FORMAT ( TotalValue / 1000000, "0.0" ) & " M"
			        )
			    RETURN
			        "Grand Total: " & FormattedTotal
			VAR PctText =
			    "Pct of GT: " & FORMAT ( [PctofTotal], "0.00%" )
			RETURN
			    ProductText & UNICHAR ( 10 ) & SegmentText & UNICHAR ( 10 ) & MeasureText
			        & UNICHAR ( 10 ) & TotalText
			        & UNICHAR ( 10 ) & PctText
		displayFolder: üí¨ Tooltips
		lineageTag: 7dda535b-5227-4185-b8db-73b905b82891

