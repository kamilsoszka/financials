	/// Dynamic title showing Sales with month-over-month comparison
	measure 'Sales (Title)' =
			
			// Get current month Sales value
			VAR CurrentValue = [Sales] // Calculate previous month Sales value
			VAR PreviousValue =
			    CALCULATE ( [Sales], PREVIOUSMONTH ( 'calendar'[Date] ) ) // Calculate difference between current and previous month
			VAR Difference = CurrentValue - PreviousValue // Helper function to format previous month value
			VAR FormatPreviousMonth =
			    VAR ValueToFormat = PreviousValue
			    VAR AbsValue =
			        ABS ( ValueToFormat )
			    VAR FormatPattern =
			        SWITCH (
			            TRUE (),
			            // Format for values up to 1,000
			            AbsValue <= 1E3, "#,##00",
			            // Format for thousands (K) - values up to 1,000,000
			            AbsValue <= 1E6, "#,##0,.00 K",
			            // Format for millions (M) - values up to 1,000,000,000
			            AbsValue <= 1E9, "#,##0,,.00 M",
			            // Format for billions (G) - values above 1,000,000,000
			            "#,##0,,,.00 G"
			        )
			    RETURN
			        FORMAT ( ValueToFormat, FormatPattern ) // Helper function to format the difference
			VAR FormatDifference =
			    VAR DiffValue =
			        ABS ( Difference )
			    VAR FormatPattern =
			        SWITCH (
			            TRUE (),
			            // Format for differences up to 1,000
			            DiffValue <= 1E3, "#,##00",
			            // Format for differences in thousands (K)
			            DiffValue <= 1E6, "#,##0,.00 K",
			            // Format for differences in millions (M)
			            DiffValue <= 1E9, "#,##0,,.00 M",
			            // Format for differences in billions (G)
			            "#,##0,,,.00 G"
			        ) // Determine arrow symbol based on direction
			    VAR Symbol =
			        SWITCH (
			            TRUE (),
			            Difference > 0, "‚ñ≤ ",
			            // Up arrow for positive difference
			            Difference < 0, "‚ñº ",
			            // Down arrow for negative difference
			            "= " // Equal sign for no change
			        )
			    RETURN
			        Symbol & FORMAT ( DiffValue, FormatPattern ) // Return formatted string
			RETURN
			    IF (
			        // Check if both values are available
			        NOT ISBLANK ( PreviousValue ) && NOT ISBLANK ( CurrentValue ),
			        // Format: "PreviousMonthValue | Arrow DifferenceValue"
			        FormatPreviousMonth & " | " & FormatDifference,
			        // Return message if data is missing
			        "No data available"
			    )
		displayFolder: üé® Formatting\Month-over-Month Titles
		lineageTag: 1c3cd058-6463-4dae-88f8-945157d36890

